import fab

title('function prefix xy')

def function_prefix_xy(x0,y0,x1,y1,fn):
   from fab.types import Shape, Transform
   return Shape(
      fn % locals(),
      x0,y0,x1,y1)

input('x0',float,-1)
input('x1',float,1)
input('y0',float,-1)
input('y1',float,1)
fn = '-Y/s*f25X*f25X'

output('shape',function_prefix_xy(x0,y0,x1,y1,fn))

